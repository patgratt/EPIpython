    "# prototype for linked list\n",
    "\n",
    "class ListNode:\n",
    "    def __init__(self, data=0, next_node=None):\n",
    "        self.data = data\n",
    "        self.next = next_node\n",
    "\n",
    "    def search_list(L, key):\n",
    "        while L and L.data != key:\n",
    "            L = L.next"
